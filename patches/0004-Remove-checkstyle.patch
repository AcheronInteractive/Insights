Subject: [PATCH] Remove checkstyle


diff --git a/build.gradle.kts b/build.gradle.kts
index 8d2297b03aca60f8795aa6dcf66b2083ac290f71..96d59109d5eddc01b0ab492b0d675104537a1a99 100644
--- a/build.gradle.kts
+++ b/build.gradle.kts
@@ -19,7 +19,6 @@ val targetMinecraftVersions = listOf("1.21.4", "1.21.5", "1.21.6", "1.21.7", "1.
 
 subprojects {
     apply(plugin = "java")
-    apply(plugin = "checkstyle")
     apply(plugin = "com.gradleup.shadow")
 
     version = rootProject.version
@@ -78,7 +77,7 @@ subprojects {
 
     tasks {
         build {
-            dependsOn("shadowJar", "checkstyleMain", "checkstyleTest", "test")
+            dependsOn("shadowJar", "test") // Insights-Patched - Remove checkstyle
         }
 
         compileJava {
@@ -101,13 +100,6 @@ subprojects {
         }
     }
 
-    tasks.withType<Checkstyle>().configureEach {
-        configFile = file("$rootDir/config/checkstyle/checkstyle.xml")
-        ignoreFailures = false
-        maxErrors = 0
-        maxWarnings = 10
-    }
-
     tasks.withType<ShadowJar> {
         relocate("dev.frankheijden.minecraftreflection", "$dependencyDir.minecraftreflection")
         relocate("io.papermc.lib", "$dependencyDir.paperlib")
